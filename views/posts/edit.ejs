<h1>EDIT POSTS PAGE</h1>
<form action="/posts/<%=post._id%>?_method=PUT" method="POST">
    <div>
        <input
            type="text"
            name="post[title]"
            placeholder="Title"
            value="<%=post.title%>"
        />
    </div>
    <div>
        <input
            type="text"
            name="post[price]"
            placeholder="Price"
            value="<%=post.price%>"
        />
    </div>
    <div>
        <textarea name="post[description]" placeholder="Description">
<%=post.description%></textarea
        >
    </div>
    <div>
        <input
            type="text"
            name="post[location]"
            placeholder="Location"
            value="<%=post.location%>"
        />
    </div>

    <button>Submit!</button>
</form>
